    </style>

    <style name="MyActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="android:background">@color/twitter_blue</item>


        <!-- Support library compatibility -->
        <item name="background">@color/twitter_blue</item>

    </style>

</resources>